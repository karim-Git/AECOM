#import "UIColor+CustomColor.h"

@implementation UIColor (CustomColor)
+(UIColor *)appWhiteColor
{
    return [UIColor whiteColor];
}
+(UIColor *)twitterBGColor
{
    return [UIColor colorWithRed:70/255.0f green:182/255.0f blue:227/255.0f alpha:1.0];
}

+(UIColor *)instaBGColor
{
    return [UIColor colorWithRed:147/255.0f green:177/255.0f blue:26/255.0f alpha:1.0];
}
+(UIColor *)cellBGColor
{
 return [UIColor colorWithRed:242/255.0f green:242/255.0f blue:242/255.0f alpha:1.0];
}
+(UIColor *)appBlackColor
{
    return [UIColor blackColor];
}
+(UIColor *)userNameColor
{
    return [UIColor colorWithRed:116/255.0f green:178/255.0f blue:225/255.0f alpha:1.0];
}
+(UIColor *)bottomViewColor
{
     return [UIColor colorWithRed:29/255.0f green:30/255.0f blue:38/255.0f alpha:1.0];
}

+(UIColor *)notificationLeftColor
{
    return [UIColor colorWithRed:235/255.0f green:145/255.0f blue:57/255.0f alpha:1.0];
}
@end
